    "            \"\"\"\n",
    "            MATCH (c:Class {qualified_name: $child_cls}), (p:Class {qualified_name: $parent_cls})\n",
    "            MERGE (c)-[:INHERITS]->(p)\n",
    "            \"\"\",\n",
    "            {\"child_cls\": child_cls, \"parent_cls\": parent_cls}\n",
    "        )\n"
